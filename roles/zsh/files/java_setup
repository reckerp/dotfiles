# Set your default JAVA_HOME
export JAVA_HOME=$(/usr/libexec/java_home -v 17.0.6)

alias j11="export JAVA_HOME=`/usr/libexec/java_home -v 11.0.16.1`; java -version"
alias j17="export JAVA_HOME=`/usr/libexec/java_home -v 17.0.6`; java -version"
alias j19="export JAVA_HOME=`/usr/libexec/java_home -v 19.0.2`; java -version"

alias jdks="ls /Users/paulrecker/Library/Java/JavaVirtualMachines"

# Add maven to path
export PATH=/opt/homebrew/Cellar/maven/3.9.0/bin:$PATH
